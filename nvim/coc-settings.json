{
    "coc.preferences.formatOnSaveFiletypes": ["python", "rust"],
    "rust-analyzer.enable": true,
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "rust-analyzer.reloadWorkspace": true,
    "rust-analyzer.updates.channel": "stable",
    "rust-analyzer.diagnostics.disabled": [
		    "macro-error"
	  ],
    "rust-analyzer.cargo.cfgs": [
        "feature1",
        "feature2"
    ],

    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.mypyEnabled": false,
    "python.linting.enabled": true,
    
    "coc.preferences.acceptSuggestionOnCommitCharacter": true,
    "coc.preferences.snippetSuggestions": "top"
}

